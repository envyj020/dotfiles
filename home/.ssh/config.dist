Host *
    SendEnv LANG LC_*
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h:%p
    ControlPersist 10m

    ServerAliveInterval 60
    ServerAliveCountMax 3
    ConnectTimeout 15

    AddKeysToAgent yes
    IdentitiesOnly yes

    HashKnownHosts no
    StrictHostKeyChecking accept-new
    ForwardAgent no

    LogLevel ERROR
